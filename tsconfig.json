{
	"compilerOptions": {
		"target": "es2020",
		"module": "CommonJS",
		"moduleResolution": "node",
		"lib": ["es2020", "DOM"],
		"baseUrl": "./",
		"rootDir": "./src",
		"outDir": "./dist",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"sourceMap": true
	}
}
